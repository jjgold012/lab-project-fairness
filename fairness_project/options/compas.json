{
  "description":  "compas dataset with protected value race",
  "data_set":     "compas-dataset",
  "file":         "compas-scores-two-years.csv",
  "data_headers": "age,sex,race,priors_count,c_charge_degree",
  "protected":    "race",
  "tag":          "is_recid",
  "fp":           true,
  "fn":           true,
  "weight":       {"gt": 0, "lt": 400},
  "weight_res":   21,
  "gamma":        {"gt": 0, "lt": 10},
  "gamma_res":    21,
  "test_size":    0.33,
  "num_of_tries": 5,

  "filters": [
    {
      "header": "c_charge_degree",
      "values": ["0"]
    },
    {
      "header": "is_recid",
      "values": ["-1"]
    },
    {
      "header": "score_text",
      "values": ["N/A"]
    },
    {
      "header": "days_b_screening_arrest",
      "values": [""],
      "range": {
        "type": "or",
        "gt": 30,
        "lt": -30
      }
    }
  ],

  "headers_integer_values": [
    {
      "header": "sex",
      "Male": 0,
      "Female": 1
    },
    {
      "header": "race",
      "Caucasian": 0,
      "African-American": 1
    },
    {
      "header": "c_charge_degree",
      "M": 0,
      "F": 1
    }
  ],
  "headers_operation": []
}